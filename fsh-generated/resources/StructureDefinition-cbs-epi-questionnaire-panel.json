  "name": "CBSEpiQuestionnairePanel",
  "title": "Case Based Surveillance Epi Questionnaire Panel",
  "status": "active",
  "experimental": true,
  "date": "2021-01-01",
  "publisher": "Georgia Tech Research Institute",
  "description": "Defines constraints on Observation to represent structured resources for the collection and organization of epidemiological questions.",
  "jurisdiction": [
    {
      "coding": [
        {
          "code": "US",
          "system": "urn:iso:std:iso:3166",
          "display": "United States of America"
        }
      ]
    }
  ],
  "fhirVersion": "4.0.1",
  "mapping": [
    {
      "identity": "workflow",
      "uri": "http://hl7.org/fhir/workflow",
      "name": "Workflow Pattern"
    },
    {
      "identity": "sct-concept",
      "uri": "http://snomed.info/conceptdomain",
      "name": "SNOMED CT Concept Domain Binding"
    },
    {
      "identity": "v2",
      "uri": "http://hl7.org/v2",
      "name": "HL7 v2 Mapping"
    },
    {
      "identity": "rim",
      "uri": "http://hl7.org/v3",
      "name": "RIM Mapping"
    },
    {
      "identity": "w5",
      "uri": "http://hl7.org/fhir/fivews",
      "name": "FiveWs Pattern Mapping"
    },
    {
      "identity": "sct-attr",
      "uri": "http://snomed.org/attributebinding",
      "name": "SNOMED CT Attribute Binding"
    }
  ],
  "kind": "resource",
  "abstract": false,
  "type": "Observation",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Observation",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Observation",
        "path": "Observation",
        "mustSupport": false
      },
      {
        "id": "Observation.status",
        "path": "Observation.status",
        "mustSupport": true
      },
      {
        "id": "Observation.category",
        "path": "Observation.category",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "coding.code"
            },
            {
              "type": "value",
              "path": "coding.system"
            }
          ],
          "ordered": false,
          "rules": "open"
        },
        "min": 1,
        "mustSupport": true
      },
      {
        "id": "Observation.category:EQCat",
        "path": "Observation.category",
        "sliceName": "EQCat",
        "min": 1,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Observation.category:EQCat.coding",
        "path": "Observation.category.coding",
        "min": 1,
        "mustSupport": true
      },
      {
        "id": "Observation.category:EQCat.coding.system",
        "path": "Observation.category.coding.system",
        "min": 1,
        "fixedUri": "http://cbsig.chai.gatech.edu/CodeSystem/cbs-temp-code-system",
        "mustSupport": true
      },
      {
        "id": "Observation.category:EQCat.coding.code",
        "path": "Observation.category.coding.code",
        "min": 1,
        "fixedCode": "epi-questionnaire",
        "mustSupport": true
      },
      {
        "id": "Observation.code",
        "path": "Observation.code",
        "short": "Coded Questionnaire Items from Surveillance Program Specified Valueset",
        "definition": "Coded Questionnaire Items from Surveillance Program Specified Valueset",
        "mustSupport": true,
        "binding": {
          "strength": "example",
          "valueSet": "http://cbsig.chai.gatech.edu/ValueSet/CBSSTDSexualHistoryDrugUseQuestionnaireVS",
          "description": "This identifies the coded questionnaire item."
        }
      },
      {
        "id": "Observation.subject",
        "path": "Observation.subject",
        "min": 1,
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://cbsig.chai.gatech.edu/StructureDefinition/cbs-patient"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Observation.value[x]",
        "path": "Observation.value[x]",
        "mustSupport": true
      },
      {
        "id": "Observation.hasMember",
        "path": "Observation.hasMember",
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://cbsig.chai.gatech.edu/StructureDefinition/cbs-epi-questionnaire-panel"
            ]
          }
        ]
      }
    ]
  }
}
