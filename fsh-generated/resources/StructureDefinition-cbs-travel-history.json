{
  "resourceType": "StructureDefinition",
  "id": "cbs-travel-history",
  "extension": [
    {
      "url": "http://hl7.org/fhir/StructureDefinition/structuredefinition-category",
      "valueString": "Clinical.Diagnostics"
    },
    {
      "url": "http://hl7.org/fhir/StructureDefinition/structuredefinition-security-category",
      "valueCode": "patient"
    }
  ],
  "url": "http://cbsig.chai.gatech.edu/StructureDefinition/cbs-travel-history",
  "version": "0.1.0",
  "name": "CaseBasedSurveillanceTravelHistory",
  "title": "Case Based Surveillance Travel History Profile",
  "status": "active",
  "experimental": true,
  "date": "2021-01-01",
  "publisher": "Georgia Tech Research Institute",
  "description": "Defines constraints on the Observation resource to capture subject Travel History in order to meet the needs of public health surveillance programs.",
  "jurisdiction": [
    {
      "coding": [
        {
          "code": "US",
          "system": "urn:iso:std:iso:3166",
          "display": "United States of America"
        }
      ]
    }
  ],
  "fhirVersion": "4.0.1",
  "mapping": [
    {
      "identity": "workflow",
      "uri": "http://hl7.org/fhir/workflow",
      "name": "Workflow Pattern"
    },
    {
      "identity": "sct-concept",
      "uri": "http://snomed.info/conceptdomain",
      "name": "SNOMED CT Concept Domain Binding"
    },
    {
      "identity": "v2",
      "uri": "http://hl7.org/v2",
      "name": "HL7 v2 Mapping"
    },
    {
      "identity": "rim",
      "uri": "http://hl7.org/v3",
      "name": "RIM Mapping"
    },
    {
      "identity": "w5",
      "uri": "http://hl7.org/fhir/fivews",
      "name": "FiveWs Pattern Mapping"
    },
    {
      "identity": "sct-attr",
      "uri": "http://snomed.org/attributebinding",
      "name": "SNOMED CT Attribute Binding"
    }
  ],
  "kind": "resource",
  "abstract": false,
  "type": "Observation",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Observation",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Observation.extension",
        "path": "Observation.extension",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "url"
            }
          ],
          "ordered": false,
          "rules": "open"
        }
      },
      {
        "id": "Observation.extension:programSpecificTimeWindow",
        "path": "Observation.extension",
        "sliceName": "programSpecificTimeWindow",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://cbsig.chai.gatech.edu/StructureDefinition/cbs-program-specific-time-window"
            ]
          }
        ]
      },
      {
        "id": "Observation.code",
        "path": "Observation.code",
        "patternCodeableConcept": {
          "coding": [
            {
              "code": "420008001",
              "system": "http://snomed.info/sct"
            }
          ]
        },
        "example": [
          {
            "label": "for Observation.code",
            "valueCodeableConcept": {
              "coding": [
                {
                  "code": "420008001",
                  "system": "http://snomed.info/sct",
                  "display": "Travel"
                }
              ],
              "text": "Travel History"
            }
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Observation.effective[x]",
        "path": "Observation.effective[x]",
        "short": "Date or period of time spent in the location",
        "mustSupport": true
      },
      {
        "id": "Observation.component",
        "path": "Observation.component",
        "min": 1,
        "mustSupport": true
      },
      {
        "id": "Observation.component.extension",
        "path": "Observation.component.extension",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "url"
            }
          ],
          "rules": "open"
        }
      },
      {
        "id": "Observation.component.extension:travelHistoryAddress",
        "path": "Observation.component.extension",
        "sliceName": "travelHistoryAddress",
        "short": "Travel history address",
        "definition": "Where the patient traveled to as an address e.g.country, city, street address if applicable. At the least, address must contain the country.",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/us/ecr/StructureDefinition/travel-history-address-extension"
            ]
          }
        ]
      },
      {
        "id": "Observation.component.code",
        "path": "Observation.component.code",
        "patternCodeableConcept": {
          "coding": [
            {
              "code": "LOC",
              "system": "http://terminology.hl7.org/CodeSystem/v3-ParticipationType",
              "display": "Location"
            }
          ]
        },
        "mustSupport": true
      },
      {
        "id": "Observation.component.value[x]",
        "path": "Observation.component.value[x]",
        "type": [
          {
            "code": "CodeableConcept"
          }
        ]
      },
      {
        "id": "Observation.component.value[x].coding",
        "path": "Observation.component.value[x].coding",
        "example": [
          {
            "label": "for coding",
            "valueCoding": {
              "code": "FJI",
              "system": "urn:iso:std:iso:3166",
              "display": "Fiji"
            }
          }
        ],
        "mustSupport": true,
        "binding": {
          "strength": "extensible",
          "valueSet": "https://phinvads.cdc.gov/fhir/ValueSet/2.16.840.1.114222.4.11.3201",
          "description": "Geographical location history"
        }
      },
      {
        "id": "Observation.component.value[x].text",
        "path": "Observation.component.value[x].text",
        "example": [
          {
            "label": "for text",
            "valueString": "Spent 8 years in the UK during the BSE outbreak"
          }
        ],
        "mustSupport": true
      }
    ]
  }
}
